{
    "builds": [{
        "src": "air_analysis/wsgi.py",
        "use": "@vercel/python",
        "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "air_analysis/wsgi.py"
        }
    ]
}